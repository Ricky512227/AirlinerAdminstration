{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "AeroPlaneName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "AeroPlaneType": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "AeroPlaneMaker": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "AeroPlaneModel": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "SeatingCapacity": {
      "type": "integer",
      "minimum": 1
    },
    "FuelCapacity": {
      "type": "integer",
      "minimum": 1
    },
    "CreatedAtTime": {
      "type": "string",
      "format": "date-time"
    },
    "UpdatedAtTime": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "AeroPlaneName",
    "AeroPlaneType",
    "AeroPlaneMaker",
    "AeroPlaneModel",
    "SeatingCapacity",
    "FuelCapacity"
  ]
}
