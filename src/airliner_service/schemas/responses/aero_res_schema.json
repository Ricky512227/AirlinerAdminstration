{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "data": {
      "type": "object",
      "properties": {
        "AeroPlaneID": {
          "type": "integer"
        },
        "AeroPlaneName": {
          "type": "string"
        },
        "AeroPlaneType": {
          "type": "string"
        },
        "AeroPlaneMaker": {
          "type": "string"
        },
        "AeroPlaneModel": {
          "type": "string"
        },
        "SeatingCapacity": {
          "type": "integer"
        },
        "FuelCapacity": {
          "type": "integer"
        },
        "CreatedAtTime": {
          "type": "string",
          "format": "date-time"
        },
        "UpdatedAtTime": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "AeroPlaneID",
        "AeroPlaneName",
        "AeroPlaneType",
        "AeroPlaneMaker",
        "AeroPlaneModel",
        "SeatingCapacity",
        "FuelCapacity",
        "CreatedAtTime",
        "UpdatedAtTime"
      ]
    }
  },
  "required": ["message", "data"]
}



